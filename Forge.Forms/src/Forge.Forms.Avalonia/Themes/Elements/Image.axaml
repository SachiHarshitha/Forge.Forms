<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:defaults="clr-namespace:Forge.Forms.AvaloniaUI.FormBuilding.Defaults"
    xmlns:formBuilding="clr-namespace:Forge.Forms.AvaloniaUI.FormBuilding">

    <ControlTemplate x:Key="ImagePresenterTemplate" TargetType="{x:Type defaults:ImagePresenter}">
        <Image
            Width="{formBuilding:FormBinding Width}"
            Height="{formBuilding:FormBinding Height}"
            HorizontalAlignment="{formBuilding:FormBinding HorizontalAlignment}"
            VerticalAlignment="{formBuilding:FormBinding VerticalAlignment}"
            Source="{formBuilding:FormBinding Source}"
            Stretch="{formBuilding:FormBinding Stretch}"
            StretchDirection="{formBuilding:FormBinding StretchDirection}" />
    </ControlTemplate>


</ResourceDictionary>